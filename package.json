{
  "private": true,
  "scripts": {
    "docker:start": "docker-compose up underbase-dev",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose run underbase-test",
    "start": "npm run build && npm run lint && npm run test",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --coverage --useStderr --forceExit --detectOpenHandles",
    "test:debug": "cross-env NODE_ENV=test node --inspect=0.0.0.0:5857 jest  --coverage --useStderr --forceExit --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --useStderr --coverage",
    "test:debug:watch": "cross-env NODE_ENV=test node --inspect=0.0.0.0:5857 jest --coverage --useStderr",
    "docs:serve": "./node_modules/docsify-cli/bin/docsify serve ./docs",
    "docs:ts": "typedoc --out ts-docs ./packages/**/*.ts --theme default --readme none --exclude './packages/underbase-cli/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/jest": "^24.0.16",
    "@types/sinon": "7.0.13",
    "cross-env": "5.2.0",
    "docsify-cli": "4.3.0",
    "husky": "3.0.2",
    "jest": "24.5.0",
    "jest-extended": "0.11.2",
    "lerna": "^3.16.4",
    "sinon": "7.3.2",
    "ts-jest": "24.0.2",
    "ts-loader": "5.4.5",
    "tslint": "5.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-microsoft-contrib": "6.1.0",
    "typedoc": "0.15.0",
    "typescript": "3.5.3",
    "vrsource-tslint-rules": "6.0.0",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-node-externals": "1.7.2"
  }
}
