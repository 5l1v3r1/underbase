{
  "name": "underbase-cli",
  "version": "1.0.5",
  "description": "A CLI tool for Underbase library",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "underbase": "./bin/index.js"
  },
  "scripts": {
    "lint": "tslint --project . -e *.js",
    "build": "webpack-cli --mode production --config ./webpack.config.js --progress --color --display-error-details",
    "build:watch": "rm -rf build && webpack --mode production --config ./webpack.config.js --watch"
  },
  "author": "RaphaÃ«l Cerveaux <raphael@crvx.fr>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sundowndev/underbase.git",
    "directory": "packages/underbase-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sundowndev/underbase/issues"
  },
  "keywords": [
    "mongodb",
    "migration",
    "schema",
    "cli"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.16.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "8.0.0",
    "@types/yargs": "^13.0.0",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "esm": "3.2.25",
    "fs-extra": "8.1.0",
    "underbase": "file:../underbase",
    "yargs": "13.3.0"
  }
}
